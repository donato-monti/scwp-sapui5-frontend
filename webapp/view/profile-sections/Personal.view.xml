<mvc:View controllerName="scwp.scwpfrontend.controller.Personal" xmlns:mvc="sap.ui.core.mvc"
	xmlns:forms="sap.ui.layout.form" xmlns="sap.m">
	<List id="_IDGenList1" headerText="General Information About Me"
		items="{path: 'profileData>/', factory: '.onListItemFactory'}">
	</List>
	<FlexBox id="_IDGenFlexBox1" class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom"
		justifyContent="Center" alignItems="Center">
		<Button id="showHiddenButton" text="Show Hidden Questions"
			press="onShowHiddenQuestionsPress" visible="{editModel>/editable}" />
	</FlexBox>
</mvc:View>